<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Desakita-Mobile</name>
    <description>
        Aplikasi Sistem Informasi Desa berbasis Android.
    </description>
    <author email="mimow.aja@gmail.com" href="http://www.momomimo.id/desakita">
        JejakDigital Mahasiswa Unisbank
    </author>
    <content src="login.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/octopus/assets/images/android/icon36.png" density="ldpi" />
        <icon src="www/octopus/assets/images/android/icon48.png" density="mdpi" />
        <icon src="www/octopus/assets/images/android/icon72.png" density="hdpi" />
        <icon src="www/octopus/assets/images/android/icon96.png" density="xhdpi" />
        <icon src="www/octopus/assets/images/android/icon144.png" density="xxdpi" />
        <icon src="www/octopus/assets/images/android/icon192.png" density="xxxdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
     <engine name="android" spec="~7.0.0" />
</widget>
